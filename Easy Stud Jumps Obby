-- // Define tab3 \\ --

tab3:Section("Spawn World")

tab3:Toggle("Grass Egg", function(state)

    if state then

           _G.loop = true

           while _G.loop == true do wait()

           local args = {

    [1] = "Grass Egg",

    [2] = 1

}

 

game:GetService("ReplicatedStorage"):WaitForChild("RecodeRemotes"):WaitForChild("BuyEgg"):InvokeServer(unpack(args))   

           end

        else

           _G.loop = false

        end

end)

