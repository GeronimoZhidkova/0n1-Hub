-- // Define tab2 \\ --

tab2:Section("Key")

tab2:Box("Enter Key","",function(Value)

   _G.KeyInput = Value 

end)

tab2:Button("Check Key", function()

    if _G.KeyInput == _G.Key then

        game:GetService("StarterGui"):SetCore("SendNotification",{

            Title = "Key System",

            Text = "You've entered the correct key!"

        })

        LoadGame()

    else

        game:GetService("StarterGui"):SetCore("SendNotification",{

            Title = "Key System",

            Text = "Wrong Key!"

        })

    end

end)

tab2:Section("Get Key")

tab2:Button("Method 1", function()

    setclipboard("https://work.ink/4m8/0n1key")

    game:GetService("StarterGui"):SetCore("SendNotification",{

            Title = "Get Key!",

            Text = "Method 1 Copied to Clipboard!"

    })

end)

