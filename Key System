-- // Boot Library \\ --

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/mag"))()

-- // Functions & Values \\ --

function Destroy()

game.CoreGui:FindFirstChild("MagikHax"):Destroy()

end

function LoadGame()

loadstring(game:HttpGet("https://raw.githubusercontent.com/GeronimoZhidkova/0n1-Hub/main/Game%20System"))()

end

_G.Key = "nz8nJoHA20x"

_G.KeyInput = "string"

